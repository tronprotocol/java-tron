<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">
  <context:component-scan base-package="org.tron"/>

  <bean id="creatorCounter" class="org.tron.stresstest.dispatch.creator.CreatorCounter"></bean>
  <bean id="dispatcher" class="org.tron.stresstest.dispatch.strategy.Dispatcher">
    <property name="source">
      <list>
        <ref bean="mutil"/>
      </list>
    </property>
  </bean>
  <bean id="level1Strategy" class="org.tron.stresstest.dispatch.strategy.Level1Strategy"/>
  <bean id="mutil" parent="level1Strategy">
    <property name="name" value="mutil"/>
    <property name="begin" value="0"/>
    <property name="end" value="99"/>
    <property name="source">
  <list>
<ref bean="MultiSignTriggerContract"/>
<ref bean="transferToken"/>
<ref bean="freezeEnergy"/>
<ref bean="exchangeInject"/>
<ref bean="MultiSignSendCoin"/>
<ref bean="participateAssetIssue"/>
<ref bean="freezeNet"/>
<ref bean="exchangeWithdraw"/>
<ref bean="niceExchangeTransaction"/>
<ref bean="niceTransferAssetTransaction"/>
<ref bean="niceTransferTransaction"/>
      </list>
    </property>
  </bean>

<bean id="MultiSignTriggerContract" class="org.tron.stresstest.dispatch.creator.MultiSign.MultiSignTriggerContract">
<property name="name" value="MultiSignTriggerContract"/>
<property name="begin" value="0"/>
<property name="end" value="9"/>
</bean>
<bean id="transferToken" class="org.tron.stresstest.dispatch.creator.contract.TransferTokenCreator">
<property name="name" value="transferToken"/>
<property name="begin" value="10"/>
<property name="end" value="51"/>
</bean>
<bean id="freezeEnergy" class="org.tron.stresstest.dispatch.creator.freeze.FreezeEnergyCreator">
<property name="name" value="freezeEnergy"/>
<property name="begin" value="52"/>
<property name="end" value="55"/>
</bean>
<bean id="exchangeInject" class="org.tron.stresstest.dispatch.creator.exchange.ExchangeInjectCreator">
<property name="name" value="exchangeInject"/>
<property name="begin" value="56"/>
<property name="end" value="57"/>
</bean>
<bean id="MultiSignSendCoin" class="org.tron.stresstest.dispatch.creator.MultiSign.MultiSignSendCoin">
<property name="name" value="MultiSignSendCoin"/>
<property name="begin" value="58"/>
<property name="end" value="67"/>
</bean>
<bean id="participateAssetIssue" class="org.tron.stresstest.dispatch.creator.asset.ParticipateAssetIssueCreator">
<property name="name" value="participateAssetIssue"/>
<property name="begin" value="68"/>
<property name="end" value="71"/>
</bean>
<bean id="freezeNet" class="org.tron.stresstest.dispatch.creator.freeze.FreezeNetCreator">
<property name="name" value="freezeNet"/>
<property name="begin" value="72"/>
<property name="end" value="75"/>
</bean>
<bean id="exchangeWithdraw" class="org.tron.stresstest.dispatch.creator.exchange.ExchangeWithdrawCreator">
<property name="name" value="exchangeWithdraw"/>
<property name="begin" value="76"/>
<property name="end" value="77"/>
</bean>
<bean id="niceExchangeTransaction" class="org.tron.stresstest.dispatch.creator.exchange.NiceExchangeTransactionCreator">
<property name="name" value="niceExchangeTransaction"/>
<property name="begin" value="78"/>
<property name="end" value="79"/>
</bean>
<bean id="niceTransferAssetTransaction" class="org.tron.stresstest.dispatch.creator.asset.NiceTransferAssetTransactionCreator">
<property name="name" value="niceTransferAssetTransaction"/>
<property name="begin" value="80"/>
<property name="end" value="89"/>
</bean>
<bean id="niceTransferTransaction" class="org.tron.stresstest.dispatch.creator.transfer.NiceTransferTransactionCreator">
<property name="name" value="niceTransferTransaction"/>
<property name="begin" value="90"/>
<property name="end" value="99"/>
</bean>

</beans>